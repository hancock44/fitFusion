<h1>New Workout</h1>

<%= form_with model: @workoutlog do |form| %>
  <div>
    <%= form.label :day %><br>
    <%= form.text_field :day %>
  </div>

  <div>
    <%= form.label :type %><br>
    <%= form.text_field :type %>
  </div>

  <div>
    <%= form.label :description %><br>
    <%= form.text_area :description %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
