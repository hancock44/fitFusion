<table class="table">
  <thead>
    <tr>
      <th scope="col">Day</th>
      <th scope="col">Type</th>
      <th scope="col"></th>
      <th scope="col"></th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
    <% @workoutlogs.each do |workoutlog| %>
      <tr>
        <td>
          <%= workoutlog.day %>
        </td>
        <td>
            <%= workoutlog.type %>
        </td>
        <td>
          <%= link_to "Show", show_workoutlog_path(workoutlog) %>
        </td>
        <td>
          <%= link_to "Destroy", workoutlog_path(workoutlog), data: {
                    turbo_method: :delete,
                    turbo_confirm: "Are you sure?"
                  } %>
        </td>
      </tr>
  </tbody>
</table>
<%= link_to "New Workout", new_workoutlog_path %>
